/**
 *   this file defined the default style sheet of window.
 */
 
QPushButton
{   
  background-color: rgba(255, 255, 255, 0%);
  border-width: 1px;
  border-color: gray;
  border-style: solid;
  border-radius: 5;
  padding: 16px;
  width: 10;
  height: 10;
}

QPushButton#pbStart
{
	border-image: url(res/icons/start.png);
}

QPushButton#pbAuto
{
	border-image: url(res/icons/auto.png);
}

QPushButton:hover {
   background-color: white;
}
